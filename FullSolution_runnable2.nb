      Sqrt[3/2] D[g, x, y, y] -  D[g, x, x, x]/Sqrt[2])/Sqrt[2]);
	 
Dcombined[g_, A_, B_] := D0[DG2m3m1A2m3[DA2m3A2m2[g]]] /. {x -> A, y -> B} // N;
	  
